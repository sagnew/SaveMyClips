{
    "manifest_version": 2,

    "name": "SaveMyClips",
    "description": "",
    "version": "1.0",

    "permissions": [
        "clipboardWrite",
        "clipboardRead"
        ],
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "background": {
        "scripts": ["clipboard.js"]
    }
}
